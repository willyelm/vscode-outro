{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark (Sundown)",
  "include": "./ui-theme.json",
  "tokenColors": [{
    "scope": "comment",
    "settings": {
			"foreground": "#7f8c98"
    }
  }, {
    "scope": [
      "keyword.operator.class",
      "keyword.operator",
      "constant.other",
      "source.php.embedded.line"
    ],
    "settings": {
      "foreground": "#599df5"
    }
  }, {
    "scope": [
      "variable", 
      "support.other.variable", 
      "string.other.link", 
      "string.regexp", 
      "entity.other.attribute-name", 
			"meta.tag", 
			"keyword.operator", 
			"constant.other.color",
      "variable.parameter",
      "declaration.tag", 
      "markup.deleted.git_gutter"
    ],
    "settings": {
			"foreground": "#f3f4f8"
    }
  }, {
		"scope": [
			"constant.numeric", 
			"constant.language", 
			"support.constant", 
			"constant.character", 
			"punctuation.section.embedded", 
			"keyword.other.unit"
		],
    "settings": {
			"foreground": "#f8e48a"
    }
  },{
    "scope": [
      "entity.name.class", 
			"support.class",
      "entity.name.type", 
      "support.type"
    ],
    "settings": {
			"foreground": "#76cef7"
    }
  }, {
    "scope": [
      "string", 
      "constant.other.symbol", 
      "entity.other.inherited-class", 
      "markup.heading", 
      "markup.inserted.git_gutter"
    ],
    "settings": {
      "foreground": "#ec8787"
    }
  }, {
		"scope": [
			"entity.name.function", 
			"meta.function-call", 
			"support.function", 
			"keyword.other.special-method", 
			"meta.block-level", 
			"markup.changed.git_gutter"
		],
    "settings": {
      "fontStyle": "",
      "foreground": "#828ce9"
    }
  }, {
    "scope": [
      "keyword", 
      "storage", 
      "storage.type",
      "entity.name.tag.css", 
      "entity.name.tag"
    ],
    "settings": {
      "fontStyle": "bold",
      "foreground": "#e471b4"
    }
  }],
  "semanticTokenColors": {
    "newOperator": "#d4d4d4",
    "stringLiteral": "#ce9178",
    "customLiteral": "#D4D4D4",
    "numberLiteral": "#b5cea8",
  }
}
