{
	"$schema": "vscode://schemas/color-theme",
	"name": "Dark (Sundown)",
	"include": "./ui-theme.json",
	"tokenColors": [{
		"name": "Comment",
		"scope": "comment",
		"settings": {
				"foreground": "#7f8c98"
		}
	}, {
		"name": "Foreground, Operator",
		"scope": [
			"keyword.operator.class",
			"keyword.operator",
			"constant.other",
			"source.php.embedded.line"
		],
		"settings": {
			"foreground": "#b3ddff"
		}
	}, {
		"name": "Variable, String Link, Regular Expression, Tag Name, GitGutter deleted",
		"scope": [
			"variable", 
			"support.other.variable", 
			"string.other.link", 
			"string.regexp", 
			"entity.other.attribute-name", 
			"meta.tag", 
			"variable.parameter",
			"declaration.tag", 
			"markup.deleted.git_gutter"
		],
		"settings": {
				"foreground": "#eeeff3"
		}
	}, {
		"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
		"scope": "constant.numeric, constant.language, support.constant, constant.character, punctuation.section.embedded, keyword.other.unit",
		"settings": {
				"foreground": "#FFC58F"
		}
	},{
		"name": "Class, Support",
		"scope": [
			"entity.name.class", 
			"entity.name.type", 
			"support.type", 
			"support.class"
		],
		"settings": {
			"fontStyle": "",
			"foreground": "#e4e283"
		}
	}, {
		"name": "String, Symbols, Inherited Class, Markup Heading, GitGutter inserted",
		"scope": [
			"string", 
			"constant.other.symbol", 
			"entity.other.inherited-class", 
			"markup.heading", 
			"markup.inserted.git_gutter"
		],
		"settings": {
				"fontStyle": "",
				"foreground": "#abe0ca"
		}
	}, {
		"name": "Operator, Misc",
		"scope": "keyword.operator, constant.other.color",
		"settings": {
				"foreground": "#8edbff"
		}
	}, {
		"name": "Function, Special Method, Block Level, GitGutter changed",
		"scope": "entity.name.function, meta.function-call, support.function, keyword.other.special-method, meta.block-level, markup.changed.git_gutter",
		"settings": {
			"fontStyle": "",
			"foreground": "#999fee"
		}
	}, {
		"name": "Keyword, Storage",
		"scope": [
			"keyword", 
			"storage", 
			"storage.type",
			"entity.name.tag.css", 
			"entity.name.tag"
		],
		"settings": {
			"fontStyle": "bold",
			"foreground": "#e2709c"
		}
	}, {
		"name": "Invalid",
		"scope": "invalid",
		"settings": {
				"fontStyle": "",
				"foreground": "#aeb3c2"
		}
	}, {
		"name": "Separator",
		"scope": "meta.separator",
		"settings": {
				"foreground": "#aeb3c2"
		}
	}, {
		"name": "Deprecated",
		"scope": "invalid.deprecated",
		"settings": {
				"fontStyle": "",
				"foreground": "#D2DDE2"
		}
	}, {
		"name": "Diff foreground",
		"scope": "markup.inserted.diff, markup.deleted.diff, meta.diff.header.to-file, meta.diff.header.from-file",
		"settings": {
				"foreground": "#D2DDE2"
		}
	}, {
		"name": "Diff insertion",
		"scope": "markup.inserted.diff, meta.diff.header.to-file",
		"settings": {
				"foreground": "#aec459"
		}
	}, {
		"name": "Diff deletion",
		"scope": "markup.deleted.diff, meta.diff.header.from-file",
		"settings": {
				"foreground": "#f76f7a"
		}
	}, {
		"name": "Diff header",
		"scope": "meta.diff.header.from-file, meta.diff.header.to-file",
		"settings": {
				"foreground": "#D2DDE2"
		}
	}, {
		"name": "Diff range",
		"scope": "meta.diff.range",
		"settings": {
				"fontStyle": "italic",
				"foreground": "#3e999f"
		}
	}, {
		"name": "Markup Quote",
		"scope": "markup.quote",
		"settings": {
				"foreground": "#FFC58F"
		}
	}, {
		"name": "Markup Lists",
		"scope": "markup.list",
		"settings": {
				"foreground": "#BBDAFF"
		}
	}, {
		"name": "Markup Styling",
		"scope": "markup.bold, markup.italic",
		"settings": {
				"foreground": "#FFC58F"
		}
	}, {
		"name": "Markup Inline",
		"scope": "markup.inline.raw",
		"settings": {
				"fontStyle": "",
				"foreground": "#fcb8bd"
		}
	}, {
		"scope": "token.info-token",
		"settings": {
				"foreground": "#91b8fa"
		}
	}, {
		"scope": "token.warn-token",
		"settings": {
				"foreground": "#f5b664"
		}
	}, {
		"scope": "token.error-token",
		"settings": {
				"foreground": "#ec5c5f"
		}
	}, {
		"scope": "token.debug-token",
		"settings": {
				"foreground": "#b267e6"
		}
	}],
	"semanticTokenColors": {
		"newOperator": "#d4d4d4",
		"stringLiteral": "#ce9178",
		"customLiteral": "#D4D4D4",
		"numberLiteral": "#b5cea8",
	}
}